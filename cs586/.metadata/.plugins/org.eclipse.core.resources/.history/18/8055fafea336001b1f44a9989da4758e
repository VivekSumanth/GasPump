package OutputClasses;

import DataStore.*;

public class GasAckMsg2 extends GasAckMsg {


	public GasAckMsg2(Data data) {
		super(data);

	}


	@Override
	public void GasAckMsg() {
		Data2 d = (Data2) data;
		System.out.println("");
		System.out.println("Total Gas Pumped in : " + d.getG() + "Gallons");
		System.out.println("");
		System.out.println("	11 : PumpGallon");
		System.out.println("	12 : StopPump");
	}
}
